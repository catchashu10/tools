# -- user customizations -------------------------------------------------------

# this is the place to override or undo settings

# increase history size
set -g history-limit 50000

# start with mouse mode enabled
set -g mouse on

# force Vi mode
#   really you should export VISUAL or EDITOR environment variable, see manual
set -g status-keys vi
set -g mode-keys vi

# no delay on Escape (critical for vim/neovim)
set -sg escape-time 10

# enable focus events (needed for vim autoread, gitgutter, etc.)
set -g focus-events on

# proper terminal and 24-bit color support
set -g default-terminal "tmux-256color"
set -sa terminal-features ',xterm-256color:RGB'

# use C-a as primary prefix and Ctrl+Space as secondary prefix
set -gu prefix2
unbind C-a
unbind C-b
set -g prefix C-a
set -g prefix2 C-Space
bind C-a send-prefix
bind C-Space send-prefix -2

# new session: prompt for name + retain current path (manual binding for tmux 3.4 compat)
bind C-c command-prompt -p "new-session" "new-session -c '#{pane_current_path}' -s '%%'" #!important

# theme switcher (prefix + T to pick a theme)
bind T display-menu -T "Select Theme" \
  "Catppuccin Mocha" m "run-shell -b '$HOME/.local/bin/tmux-theme catppuccin-mocha'" \
  "Cyberpunk"        c "run-shell -b '$HOME/.local/bin/tmux-theme cyberpunk'" \
  "Dracula"          d "run-shell -b '$HOME/.local/bin/tmux-theme dracula'" \
  "Everforest"       e "run-shell -b '$HOME/.local/bin/tmux-theme everforest'" \
  "Gruvbox"          g "run-shell -b '$HOME/.local/bin/tmux-theme gruvbox'" \
  "Kanagawa"         k "run-shell -b '$HOME/.local/bin/tmux-theme kanagawa'" \
  "Nord"             n "run-shell -b '$HOME/.local/bin/tmux-theme nord'" \
  "Rose Pine"        r "run-shell -b '$HOME/.local/bin/tmux-theme rose-pine'" \
  "Tokyo Night"      t "run-shell -b '$HOME/.local/bin/tmux-theme tokyo-night'" \
  "" \
  "Mono Amber"       a "run-shell -b '$HOME/.local/bin/tmux-theme mono-amber'" \
  "Mono Blue"        b "run-shell -b '$HOME/.local/bin/tmux-theme mono-blue'" \
  "Mono Green"       h "run-shell -b '$HOME/.local/bin/tmux-theme mono-green'" \
  "Mono Purple"      p "run-shell -b '$HOME/.local/bin/tmux-theme mono-purple'" \
  "Mono Rose"        s "run-shell -b '$HOME/.local/bin/tmux-theme mono-rose'" \
  "" \
  "Default"          o "run-shell -b '$HOME/.local/bin/tmux-theme default'"

# display a message after toggling mouse support
bind m run "cut -c3- '#{TMUX_CONF}' | sh -s _toggle_mouse" \; display 'mouse #{?#{mouse},on,off}'

# scroll direction toggle (prefix + S to swap between normal/natural scrolling)
# normal scrolling = scroll down ‚Üí content goes down (mouse wheel)
# natural scrolling = scroll down ‚Üí content goes up (trackpad)
bind S if-shell "tmux show-environment -g NATURAL_SCROLL 2>/dev/null | grep -q on" \
  "set-environment -g NATURAL_SCROLL off ; \
   bind-key -T root WheelUpPane if-shell -F '#{||:#{pane_in_mode},#{mouse_any_flag}}' { send-keys -M } { copy-mode -e } ; \
   bind-key -T copy-mode WheelUpPane select-pane \\; send-keys -X -N 5 scroll-up ; \
   bind-key -T copy-mode WheelDownPane select-pane \\; send-keys -X -N 5 scroll-down ; \
   bind-key -T copy-mode-vi WheelUpPane select-pane \\; send-keys -X -N 5 scroll-up ; \
   bind-key -T copy-mode-vi WheelDownPane select-pane \\; send-keys -X -N 5 scroll-down ; \
   display 'scroll: normal (mouse wheel)'" \
  "set-environment -g NATURAL_SCROLL on ; \
   bind-key -T root WheelDownPane if-shell -F '#{||:#{pane_in_mode},#{mouse_any_flag}}' { send-keys -M } { copy-mode -e } ; \
   bind-key -T copy-mode WheelUpPane select-pane \\; send-keys -X -N 5 scroll-down ; \
   bind-key -T copy-mode WheelDownPane select-pane \\; send-keys -X -N 5 scroll-up ; \
   bind-key -T copy-mode-vi WheelUpPane select-pane \\; send-keys -X -N 5 scroll-down ; \
   bind-key -T copy-mode-vi WheelDownPane select-pane \\; send-keys -X -N 5 scroll-up ; \
   display 'scroll: natural (trackpad)'"

# move status line to top
#set -g status-position top


# -- tpm -----------------------------------------------------------------------

# while I don't use tpm myself, many people requested official support so here
# is a seamless integration that automatically installs plugins in parallel

# whenever a plugin introduces a variable to be used in 'status-left' or
# 'status-right', you can use it in 'tmux_conf_theme_status_left' and
# 'tmux_conf_theme_status_right' variables.

# by default, launching tmux will update tpm and all plugins
#   - true (default)
#   - false
tmux_conf_update_plugins_on_launch=false

# by default, reloading the configuration will update tpm and all plugins
#   - true (default)
#   - false
tmux_conf_update_plugins_on_reload=false

# by default, reloading the configuration will uninstall tpm and plugins when no
# plugins are enabled
#   - true (default)
#   - false
tmux_conf_uninstall_plugins_on_reload=false

# /!\ the tpm bindings differ slightly from upstream:
#   - installing plugins: <prefix> + I
#   - uninstalling plugins: <prefix> + Alt + u
#   - updating plugins: <prefix> + u

# /!\ do not add set -g @plugin 'tmux-plugins/tpm'
# /!\ do not add run '~/.tmux/plugins/tpm/tpm'

# /!\ the 'set -g @tpm_plugins' syntax is not supported
# to enable a plugin, use the 'set -g @plugin' syntax:
# visit https://github.com/tmux-plugins for available plugins
#set -g @plugin 'tmux-plugins/tmux-copycat'
#set -g @plugin 'tmux-plugins/tmux-cpu'
set -g @plugin 'tmux-plugins/tmux-resurrect'
set -g @plugin 'tmux-plugins/tmux-continuum'
set -g @plugin 'tmux-plugins/tmux-yank'
set -g @continuum-restore 'on'
set -g @continuum-save-interval '10'
set -g @resurrect-capture-pane-contents 'on'


# -- custom variables ----------------------------------------------------------

# to define a custom #{foo} variable, define a POSIX shell function between the
# '# EOF' and the '# "$@"' lines. Please note that the opening brace { character
# must be on the same line as the function name otherwise the parse won't detect
# it.
#
# then, use #{foo} in e.g. the 'tmux_conf_theme_status_left' or the
# 'tmux_conf_theme_status_right' variables.

# ------------------------------------------------------------------------------

# /!\ do not remove the following line
# EOF
#
# /!\ do not "uncomment" the functions: the leading "# " characters are needed
#
# # usage: #{git_info}
# # shows git branch with nerd font icon + fish-style status symbols
# #   + = staged, ! = modified, ? = untracked, ‚á°N = ahead, ‚á£N = behind
# git_info() {
#   pane_path=$(tmux display-message -p '#{pane_current_path}')
#   cd "$pane_path" 2>/dev/null || return
#   branch=$(git symbolic-ref --short HEAD 2>/dev/null) \
